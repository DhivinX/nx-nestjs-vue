{
  "name": "nx-nestjs-vue",
  "version": "0.0.0",
  "license": "MIT",
  "private": true,
  "main": "dist/apps/web/electron/main/index.js",
  "scripts": {
    "apps:dev": "nx run-many --target=serve --projects=web,server",
    "web:dev": "nx serve web",
    "web:electron:dev": "nx electron web",
    "web:electron:build": "nx electron web --configuration=build",
    "mobile:dev": "nx serve mobile",
    "mobile:android": "nx build mobile && nx run mobile:sync:android && nx run mobile:open:android",
    "server:dev": "nx serve server",
    "server:seed": "node dist/apps/server/cli.js seed",
    "build": "npm run clean && nx run-many --target=lint,test,build --configuration=production --projects=web,server",
    "test": "nx run-many --target=test --verbose",
    "clean": "rimraf dist",
    "lint": "nx run-many --target=lint",
    "docker": "npm run build && docker-compose up --force-recreate --build -d && docker image prune -f",
    "dep-graph": "node tools/scripts/patch-graph.js && nx graph"
  },
